<launch>
 
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find kobuki_i2r)/laser/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find kobuki_i2r)/laser/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find kobuki_i2r)/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find kobuki_i2r)/param/global_costmap_params.yaml" command="load" />
  </node>

</launch>
